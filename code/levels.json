{
"prefixes":{
  "foaf": "http://xmlns.com/foaf/0.1/",
  "xsd": "http://www.w3.org/2001/XMLSchema#",
  "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
  "dc": "http://purl.org/dc/elements/1.1/",
  "dct": "http://purl.org/dc/terms/",
  "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
  "skos": "http://www.w3.org/2004/02/skos/core#",
  "owl": "http://www.w3.org/2002/07/owl#"
},
"levels":{
  "description":[
    {"conceptScheme": {
      "sources": [["description.csv~csv"]],
      "s": "specificTaxonomy:",
      "po": [
        [
          "a",
          "skos:conceptScheme"
        ],
        [
          "dc:creator",
          "$(creator)"
        ],
        [
          "dc:date",
          "$(date)"
        ],
        [
          "dc:title",
          "$(title)",
          "$LANG~lang"
        ],
        [
          "skos:prefLabel",
          "$(name)",
          "$LANG~lang"
        ]
        ]
    }}    
  ],
    "level_0":[
      {"topConcept": {
        "sources": [["data.csv~csv"]],
        "s": "specificTaxonomy:",
        "po": [
          [
            "skos:hasTopConcept",
            "specificTaxonomy:$(level_0)~iri"
          ]
          ]
      }},
      {
        "label": {
          "sources": [
            [
              "labels.csv~csv"
            ]
          ],
          "s": "specificTaxonomy:$(id)",
          "po": [
            [
              "a",
              "skos:Concept"
            ],
            [
              "skos:inScheme",
              "specificTaxonomy:~iri"
            ],
            [
              "skos:prefLabel",
              "$(denomEs)",
              "es~lang"
            ],
            [
              "skos:prefLabel",
              "$(denomEn)",
              "en~lang"
            ],
            [
              "skos:definitionLabel",
              "$(defEs)",
              "es~lang"
            ],
            [
              "skos:definitionLabel",
              "$(defEn)",
              "en~lang"
            ]
          ]
        }
      },
         {
        "jerarquiaCapitulo": {
            "sources": [
              [
                "data.csv~csv"
              ]
            ],
            "s": "specificTaxonomy:$(level_0)",
            "po": [
              {
                "p": "skos:narrower",
                "o": [
                  {
                    "mapping": "label",
                    "condition": {
                      "function": "equal",
                      "parameters": [
                        [
                          "str1",
                          "$(level_1)"
                        ],
                        [
                          "str2",
                          "$(id)"
                        ]
                      ]
                    }
                  }
                ]
              }
            ]
          }
        }
        ],
        "level_1":[
            {
                "jerarquiaArticulo": {
                    "sources": [
                      [
                        "data.csv~csv"
                      ]
                    ],
                    "s": "specificTaxonomy:$(level_1)",
                    "po": [
                      {
                        "p": "skos:broader",
                        "o": [
                          {
                            "mapping": "label",
                            "condition": {
                              "function": "equal",
                              "parameters": [
                                [
                                  "str1",
                                  "$(level_0)"
                                ],
                                [
                                  "str2",
                                  "$(id)"
                                ]
                              ]
                            }
                          }
                        ]
                      },
                      {
                        "p": "skos:narrower",
                        "o": [
                          {
                            "mapping": "label",
                            "condition": {
                              "function": "equal",
                              "parameters": [
                                [
                                  "str1",
                                  "$(level_2)"
                                ],
                                [
                                  "str2",
